# Source Prezto.
if [[ -s ~/.zprezto/init.zsh ]]; then
  source ~/.zprezto/init.zsh
else
  echo "Error sourcing zpresto!"
fi

# Customize to your needs...
export KEYTIMEOUT=0.01

# common configuration
source ~/.term_common

# User configuration
# ROS

export ROS_PACKAGE_PATH="$ROS_PACKAGE_PATH:/home/arnaud/src/ext/ORB_SLAM2/Examples/ROS/ORB_SLAM2"

export PATH=$PATH:$HOME/scripts:/usr/local/bin:/usr/local/MATLAB/R2015a/bin
# export MANPATH="/usr/local/man:$MANPATH"

# You may need to manually set your language environment
# export LANG=en_US.UTF-8

# Preferred editor for local and remote sessions
if [[ -n $SSH_CONNECTION ]]; then
  export VISUAL='vim'
  export EDITOR='vim'
else
  export EDITOR='nvim'
  export VISUAL='nvim'
  alias vim='nvim'
fi

if [ "$(lsb_release -r -s)" = "16.04" ]; then
  echo "Using Ubuntu 16.04"
  echo "Activating python virtualenv ~/src16/python_venv/bin/activate"
  source ~/src16/python_venv/bin/activate
  echo "Setting up ros kinetic environment"
  source /opt/ros/kinetic/setup.zsh
fi

if [ "$(lsb_release -r -s)" = "14.04" ]; then
  echo "Using Ubuntu 14.04"
  echo "Setting up ros indigo environment"
  source /opt/ros/indigo/setup.zsh
fi
